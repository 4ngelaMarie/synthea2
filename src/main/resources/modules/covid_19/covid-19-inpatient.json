      "direct_transition": "End_Condition"
    },
    "End_Condition": {
      "codes": [
        {
          "system": "SNOMED-CT",
          "code": "840539006",
          "display": "COVID-19"
        }
      ],
      "type": "ConditionEnd",
      "direct_transition": "Hospital_Discharge"
    },
    "Hospital_Discharge": {
      "type": "EncounterEnd",
      "direct_transition": "Reset_Comorbidity_Attribute"
    },
    "Reset_Comorbidity_Attribute": {
      "attribute": "Covid-19 Comorbidity",
      "type": "SetAttribute",
      "direct_transition": "Terminal"
    }
  }
}