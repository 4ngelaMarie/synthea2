      "encounter_class": "emergency",
      "wellness": false,
      "codes": [
        {
          "system": "SNOMED-CT",
          "code": "185347001",
          "display": "Encounter for problem"
        }
      ],
      "type": "Encounter",
      "direct_transition": "Metabolic_Panel"
    },
    "Metabolic_Panel": {
      "submodule": "testing/record_comprehensive_metabolic_panel",
      "type": "CallSubmodule",
      "direct_transition": "Suspected COVID"
    },
    "Suspected COVID": {
      "submodule": "testing/covid-19-testing",
      "type": "CallSubmodule",
      "direct_transition": "Chest CT Scan"
    },
    "Chest CT Scan": {
      "duration": {
        "low": 20.0,
        "high": 90.0,
        "unit": "minutes"
      },
      "reason": "Covid-19 Condition",
      "codes": [
        {
          "system": "SNOMED-CT",
          "code": "418891003",
          "display": "Computed tomography of chest and abdomen"
        }
      ],
      "type": "Procedure",
      "lookup_table_transition": [
        {
          "transition": "Transfer_To_ICU",
          "lookup_table_name": "covid-19-admission-outcomes.csv",
          "default_probability": 0.2
        },
        {
          "transition": "Transfer_To_Ward",
          "lookup_table_name": "covid-19-admission-outcomes.csv",
          "default_probability": 0.8
        }
      ]
    },
    "Transfer_To_ICU": {
      "submodule": "covid_19/covid-19-icu-treatment",
      "type": "CallSubmodule",
      "direct_transition": "Covid-19_Recovery"
    },
    "Transfer_To_Ward": {
      "submodule": "covid_19/covid-19-ward-treatment",
      "type": "CallSubmodule",
      "direct_transition": "Covid-19_Recovery"
    },
    "Covid-19_Recovery": {
      "vital_sign": "Temperature",
      "range": {
        "low": 98.5,
        "high": 98.7,
        "unit": "F"
      },
      "unit": "F",
      "codes": [
        {
          "system": "LOINC",
          "code": "8310-5",
