  "states": {
    "Initial": {
      "type": "Initial",
      "direct_transition": "Develop_ARDS"
    },
    "Terminal": {
      "type": "Terminal"
    },
    "Develop_ARDS": {
      "codes": [
        {
          "system": "SNOMED-CT",
          "code": "65710008",
          "display": "Acute respiratory failure (disorder)"
        }
      ],
      "type": "ConditionOnset",
      "direct_transition": "Order_Mechanical_Ventilator"
    },
    "Order_Mechanical_Ventilator": {
      "assign_to_attribute": "Mechanical Ventilator",
      "used": true,
      "codes": [
        {
          "system": "SNOMED-CT",
          "code": "C0042497",
          "display": "Mechanical Ventilator"
        }
      ],
      "type": "DeviceOrder",
      "direct_transition": "ICU_Medications"
    },
    "ICU_Medications": {
      "administered": true,
      "codes": [
        {
          "system": "RxNorm",
          "code": "251854",
          "display": "Clarithromycin 50 MG/ML Injectable Suspension"
        }
      ],
      "type": "MedicationOrder",
      "direct_transition": "ICU_Covid-19_Recovery"
    },
    "ICU_Covid-19_Recovery": {
      "range": {
        "low": 2.0,
        "high": 21.0,
        "unit": "days"
      },
      "type": "Delay",
      "direct_transition": "Determine_ICU_Outcome"
    },
    "Determine_ICU_Outcome": {
      "type": "Simple",
      "lookup_table_transition": [
        {
          "transition": "ICU_Death",
          "lookup_table_name": "covid-19-ICU-outcomes.csv",
          "default_probability": 0.2
        },
        {
          "transition": "End_Ventilator",
          "lookup_table_name": "covid-19-ICU-outcomes.csv",
          "default_probability": 0.8
        }
      ]
    },
    "ICU_Death": {
      "referenced_by_attribute": "Mechanical Ventilator",
      "type": "DeviceEnd",
      "direct_transition": "ICU_Reset_Attribute"
    },
    "ICU_Reset_Attribute": {
      "attribute": "Covid-19 Comorbidity",
      "type": "SetAttribute",
      "direct_transition": "ICU_Decease"
    },
    "ICU_Decease": {
      "codes": [
        {
          "system": "SNOMED-CT",
          "code": "840539006",
          "display": "COVID-19"
        }
      ],
      "type": "Death",
      "direct_transition": "Terminal"
    },
    "End_Ventilator": {
      "referenced_by_attribute": "Mechanical Ventilator",
      "type": "DeviceEnd",
      "direct_transition": "End_ARDS"
    },
    "End_ARDS": {
      "codes": [
        {
          "system": "SNOMED-CT",
          "code": "65710008",
          "display": "Acute respiratory failure (disorder)"
        }
      ],
      "type": "ConditionEnd",
      "direct_transition": "End_Rx"
    },
    "End_Rx": {
      "codes": [
        {
          "system": "RxNorm",
          "code": "251854",
          "display": "Clarithromycin 50 MG/ML Injectable Suspension"
        }
      ],
      "type": "MedicationEnd",
      "direct_transition": "Terminal"
    }
  }
}