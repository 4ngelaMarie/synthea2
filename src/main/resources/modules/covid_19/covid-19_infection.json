  ],
  "states": {
    "Initial": {
      "type": "Initial",
      "direct_transition": "Condition_Onset"
    },
    "Terminal": {
      "type": "Terminal"
    },
    "Condition_Onset": {
      "assign_to_attribute": "Covid-19 Condition",
      "codes": [
        {
          "system": "SNOMED-CT",
          "code": "840539006",
          "display": "COVID-19"
        }
      ],
      "type": "ConditionOnset",
      "direct_transition": "Assess_Comorbidities"
    },
    "Assess_Comorbidities": {
      "submodule": "covid_19/assess-comorbidities",
      "type": "CallSubmodule",
      "direct_transition": "Symptom_Onset_Delay",
      "remarks": [
        "Assess patient comorbidities and set attribute (Covid-19 Comorbidity):",
        "  - (Yes) if patient has comorbidity impacting risks",
        "  - (No) if patient has no comorbidity impacting risks"
      ]
    },
    "Symptom_Onset_Delay": {
      "range": {
        "low": 2.0,
        "high": 14.0,
        "unit": "days"
      },
      "type": "Delay",
      "direct_transition": "Determine_Severity"
    },
    "Determine_Severity": {
      "type": "Simple",
      "lookup_table_transition": [
        {
          "transition": "Mild_Infection",
          "lookup_table_name": "covid-19-infection-outcomes.csv",
          "default_probability": 0.8
        },
        {
          "transition": "Seeks_Medical_Care",
          "lookup_table_name": "covid-19-infection-outcomes.csv",
          "default_probability": 0.2
        }
      ],
      "remarks": [
        "https://www.hopkinsguides.com/hopkins/view/Johns_Hopkins_ABX_Guide/540747/all/Coronavirus_COVID_19__SARS_2_CoV_"
      ]
    },
    "Mild_Infection": {
      "attribute": "Covid-19 Severity",
      "value": "Asymptomatic/Mild",
      "type": "SetAttribute",
      "direct_transition": "Call_Mild_Infection"
    },
    "Call_Mild_Infection": {
      "submodule": "covid_19/develop-mild-infection",
      "type": "CallSubmodule",
      "direct_transition": "Delay_To_Retain_Resistance"
    },
    "Seeks_Medical_Care": {
      "type": "Simple",
      "lookup_table_transition": [
        {
          "transition": "Outpatient",
          "lookup_table_name": "covid-19-testing-outcomes.csv",
          "default_probability": 0.8
        },
        {
          "transition": "Inpatient",
          "lookup_table_name": "covid-19-testing-outcomes.csv",
          "default_probability": 0.2
        }
      ],
      "remarks": [
        "https://www.nejm.org/doi/full/10.1056/NEJMoa2002032"
      ]
    },
    "Outpatient": {
      "attribute": "Covid-19 Severity",
      "value": "Outpatient",
      "type": "SetAttribute",
      "direct_transition": "Call_Outpatient"
    },
    "Call_Outpatient": {
      "submodule": "covid_19/covid-19-outpatient",
      "type": "CallSubmodule",
      "direct_transition": "Delay_To_Retain_Resistance"
    },
    "Inpatient": {
      "attribute": "Covid-19 Severity",
      "value": "Inpatient",
      "type": "SetAttribute",
      "direct_transition": "Call_Inpatient"
    },
    "Call_Inpatient": {
      "submodule": "covid_19/covid-19-inpatient",
      "type": "CallSubmodule",
      "direct_transition": "Delay_To_Retain_Resistance"
    },
    "Delay_To_Retain_Resistance": {
      "exact": {
        "quantity": 52,
        "unit": "weeks"
      },
      "type": "Delay",
      "direct_transition": "Terminal"
    }
  }
}