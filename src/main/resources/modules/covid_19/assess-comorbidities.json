  "states": {
    "Initial": {
      "type": "Initial",
      "direct_transition": "Assess_Comorbidities"
    },
    "Terminal": {
      "type": "Terminal"
    },
    "Assess_Comorbidities": {
      "type": "Simple",
      "conditional_transition": [
        {
          "condition": {
            "condition_type": "Or",
            "conditions": [
              {
                "condition_type": "Active Condition",
                "referenced_by_attribute": "copd_variant"
              },
              {
                "condition_type": "Active Condition",
                "referenced_by_attribute": "Cystic Fibrosis condition"
              },
              {
                "condition_type": "Active Condition",
                "referenced_by_attribute": "HIV condition"
              },
              {
                "condition_type": "Active Condition",
                "referenced_by_attribute": "Hypertension condition"
              },
              {
                "condition_type": "Active Condition",
                "referenced_by_attribute": "Pertussis condition"
              },
              {
                "condition_type": "Active Condition",
                "referenced_by_attribute": "Diabetes Mellitus"
              },
              {
                "condition_type": "Active Condition",
                "referenced_by_attribute": "Lung Cancer condition"
              },
              {
                "condition_type": "Active Condition",
                "referenced_by_attribute": "Cardiovascular Disease condition"
              }
            ]
          },
          "transition": "Set_Comorbidity"
        },
        {
          "transition": "Set_No_Comorbidity"
        }
      ]
    },
    "Set_Comorbidity": {
      "attribute": "Covid-19 Comorbidity",
      "value": "Yes",
      "type": "SetAttribute",
      "direct_transition": "Terminal"
    },
    "Set_No_Comorbidity": {
      "attribute": "Covid-19 Comorbidity",
      "value": "No",
      "type": "SetAttribute",
      "direct_transition": "Terminal"
    }
  }
}