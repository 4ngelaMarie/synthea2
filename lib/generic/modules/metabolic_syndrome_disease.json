                "operator": ">=",
                "quantity": 18,
                "unit": "years"
              }
            ]
          },
          "transition" : "Chance_to_Onset_Hypertension"
        },
        {
          "condition" : {
            "condition_type" : "Vital Sign",
            "vital_sign" : "blood_glucose",
            "operator" : ">",
            "value" : 6.5
          },
          "transition" : "Onset_Diabetes"
        },
        {
          "condition" : {
            "condition_type" : "Vital Sign",
            "vital_sign" : "blood_glucose",
            "operator" : ">",
            "value" : 5.7
          },
          "transition" : "Onset_Prediabetes"
        },
        {
          "transition" : "Potential_Diabetes"
        }
      ]
